{
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/6d52e229-7b09-4772-885e-edd85950a304/v2.0"
        }
      }
    }
  },
  "routes": [
    {
      "route": "/.auth/login/aad",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/.auth/login/aad/callback",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/*",
      "allowedRoles": [ "authenticated" ]
    }
  ],
  "navigationFallback": {
    "rewrite": "/.auth/login/aad",
    "exclude": [ "/.auth/*" ]
  }
}
